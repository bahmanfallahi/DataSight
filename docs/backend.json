{
  "entities": {
    "report": {
      "title": "Report",
      "description": "A saved data analysis report.",
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "The name of the report file."
        },
        "csvData": {
          "type": "string",
          "description": "The full CSV content of the report."
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "description": "The timestamp when the report was saved."
        }
      },
      "required": ["name", "csvData", "createdAt"]
    }
  },
  "auth": {
    "providers": []
  },
  "firestore": {
    "/reports/{reportId}": {
      "schema": { "$ref": "#/entities/report" },
      "description": "Stores user-saved data analysis reports."
    }
  }
}
